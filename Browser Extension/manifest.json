{
  "manifest_version": 2,

  "name": "SE31520 Extension",
  "description": "",
  "version": "1.0",
  "icons": {
    "128": "./img/128.png" ,
    "64" : "./img/64.png",
    "32" : "./img/32.png"
  },
  "background": {
    "scripts": [
      "./bower_components/jquery/dist/jquery.min.js", 
      "./bower_components/moment/moment.js",
      "./libs/utils.js",
      "./libs/resttoolkit.js",
      "./libs/localstorage.js",
      "./js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title" : "SE31520 Extension",
    "default_icon": "./img/128.png",
    "default_popup": "./html/index.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "alarms"
  ],
  "web_accessible_resources": [
    "./img/128.png"
  ]
}
